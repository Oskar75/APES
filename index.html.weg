<div class="home">

  <h1>Topics</h1>

  <ul class="topics">
    {% for page in site.pages %} {% if page.tags contains 'main' %}
      <li>
        <span class="post-date">{{ post.date | date: "%b %-d, %Y" }}</span>
        <a class="post-link" href="{{ page.url }}">{{ page.title }}</a>
      </li>
    {% endif %} {% endfor %}
  </ul>

  <!-- <p class="rss-subscribe">subscribe <a href="{{ "/feed.xml" }}">via RSS</a></p> -->


</div>
